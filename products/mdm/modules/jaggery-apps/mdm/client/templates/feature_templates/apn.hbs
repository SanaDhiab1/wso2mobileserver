<div class="feature-dialog">

	<div class="modal-header">
		<h4 id="myModalLabel">APN Configuration</h4>
	</div>
	<div class="modal-body">
		
		<div class="raw">
			<div class="feature-lable">Carrier: </div><input type="text" id="carrier" class="feature-input">
		</div>
		<div class="raw">
			<div class="feature-lable">Username: </div><input type="text" id="user_name" class="feature-input">
		</div>
		<div class="raw">
			<div class="feature-lable">Password: </div><input type="password" id="password" class="feature-input">
		</div>
		<div class="raw">
			<div class="feature-lable">Proxy Server: </div><input type="text" id="proxy_server" class="feature-input">
		</div>
		<div class="raw">
			<div class="feature-lable">Proxy Port: </div><input type="text" id="proxy_port" class="feature-input">
		</div>
	
	</div>

	<div class="modal-footer">
		<button class="btn btn-cancel"  data-dismiss="modal" aria-hidden="true">
			Close
		</button>
		<button  data-dismiss="modal" aria-hidden="true" class="btn btn-orange feature-command">Set APN</button>
	</div>

</div>


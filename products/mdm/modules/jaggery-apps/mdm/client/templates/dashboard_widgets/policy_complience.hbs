

<script type="text/javascript">
	var colors = {
		"Non Compliance" : "#38b5bb",
		"Compliance" : "#086F76"
	};

	for ( i = 0; i < chartData.length; i++) {
		if (colors[chartData[i].label]) {
			chartData[i].color = colors[chartData[i].label];
		}		
	}

	$.plot('#chart-{{index}}', chartData, {
		series : {
			pie : {
				show : true
			}
		},
		legend : {
			show : false
		}
	});

</script>

<div class="block">
	<div class="block-header">
		<div class="block-header-title pull-left">
			{{title}}
		</div>
	</div>

	<div class="container-fluid raw-fluid block-body" >
		<a href="/mdm/reports/devices_complience?dashboard=true&startdate=2000-01-01&enddate={{currentDate}}&status="><div id="chart-{{index}}" style="height: {{height}}; width:{{width}}"></div></a>
	</div>

</div>

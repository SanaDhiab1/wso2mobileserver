{
    "HTTPS_URL": "%https.ip%",
    "HTTP_URL": "%http.ip%",
    "IOS_URL": "%https.ip%/devicelogin.html",
    "MDM_UI_URI": "/mdm/",
    "MDM_API_URI": "/mdm/api/",
    "MDM_THEME": "wso2sinine",
    "tenanted": true,
    "permissions": {
        "login": {
            "/permission/admin/login": [
                "ui.execute"
            ]
        }
    },
    "ssoConfiguration": {
        "enabled": true,
        "issuer": "mdm",
        "identityProviderURL": "%https.ip%/sso/samlsso.jag",
        "keyStorePassword": "wso2carbon",
        "identityAlias": "wso2carbon",
        "responseSigningEnabled": "true",
        "storeAcs": "%https.ip%/mdm/acs",
        "keyStoreName": "/repository/resources/security/wso2carbon.jks"
    },
    "email": {
        "smtp":"smtp.gmail.com",
        "port":"25",
        "companyName": "WSO2",
        "senderAddress": "$(senderAddress)",
        "emailPassword": "$(senderPassword)",
        "emailTemplate": "\nYou have been registered to the WSO2Mobile MDM. Below is the link to enroll.\n \n"
    },
    "device": {
        "android_location": "%http.ip%/mdm/client_app/MDMAgent1.0.apk",
        "ios": {
            "location": "%http.ip%/mdm/client_app/iOS-MDM-Agent.ipa",
            "bundleid": "com.wso2.mobile",
            "version": "1.0",
            "appname": "MDM Agent"
        }
    },
    "mam": {
        "store_location": "%https.ip%/store/"
    },
    "monitor_interval" : "2"
}